{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "CodeLens (Launch) - PetClinicApplication",
            "request": "launch",
            "mainClass": "org.springframework.samples.petclinic.PetClinicApplication",
            "projectName": "spring-petclinic-jpa"
        },
        {
            "type": "java",
            "name": "CodeLens (Launch) - MySQL PetClinicApplication",
            "request": "launch",
            "mainClass": "org.springframework.samples.petclinic.PetClinicApplication",
            "projectName": "spring-petclinic-jpa",
            "vmArgs": "-DMYSQL_URL=jdbc:mysql://mysql/petclinic -Dspring.profiles.active=mysql"
        },
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "miDebuggerPath": "gdb",
            "program": "${workspaceFolder}/target/spring-petclinic-jpa",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "sourceFileMap": {
                "org/springframework/samples": "${workspaceFolder}/src/main/java/org/springframework/samples"
            },
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
